{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi":"off",
        "no-unused-vars":"off",
        "space-before-function-paren":"off",
        "indent":"off",
        "quotes":"off",
        "no-undef":"off"
    }
}